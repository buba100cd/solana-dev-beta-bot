{
  "name": "solana-dev-beta-trading-bot",
  "version": "1.0.0",
  "description": "Advanced Solana trading bot utilizing Solana Dev Beta features",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "start:mainnet": "set NODE_ENV=mainnet && node src/index.js",
    "start:devnet": "set NODE_ENV=devnet && node src/index.js",
    "start:local": "set NODE_ENV=local && node src/index.js",
    "test": "node --experimental-vm-modules tests/test-runner.js && npx jest --passWithNoTests",
    "test:unit": "npx jest tests/unit --verbose",
    "lint": "eslint src/",
    "deploy:dev": "node scripts/deploy-dev.js"
  },
  "keywords": [
    "solana",
    "trading-bot",
    "dev-beta",
    "mev",
    "defi"
  ],
  "author": "Solana Trading Bot Team",
  "license": "MIT",
  "dependencies": {
    "@solana/actions": "^1.6.6",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.87.0",
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "winston": "^3.18.3",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^25.0.0",
    "eslint": "^9.39.1",
    "jest": "^25.0.0",
    "nodemon": "^3.1.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
